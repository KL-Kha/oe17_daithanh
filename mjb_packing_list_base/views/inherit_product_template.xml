<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="inherited_product_template_form" model="ir.ui.view">
            <field name="name">inherited.product.template.form</field>
            <field name="model">product.template</field>
            <field name="inherit_id" ref="product.product_template_only_form_view"/>
            <field name="arch" type="xml">
                <xpath expr="//form[@name='Product Template']/sheet[1]/notebook[1]" position="inside">
                    <page name="studio_page_Za2B6" string="Cartons and pallets">
                        <field name="carton_ids">
                            <tree string="Packing List Ctn Type">
                            <field name="sequence"/>
                            <field name="name"/>
                            <field name="create_date" optional="hide"/>
                            <field name="code"/>
                            <field name="type"/>
                            <field name="cbm"/>
                            <field name="width"/>
                            <field name="length"/>
                            <field name="height"/>
                            <field name="partner_id" optional="hide"/>
                            <field name="product_id" optional="hide"/>
                            <field name="active" optional="hide"/>
                            <field name="id" optional="hide"/>
                            <field name="write_uid" optional="hide"/>
                            <field name="write_date" optional="hide"/>
                            </tree>
                        </field>
                    </page>
                </xpath>
            </field>
        </record>
    </data>
</odoo>
