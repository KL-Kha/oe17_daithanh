<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="inherited_view_move_form" model="ir.ui.view">
            <field name="name">inherited.view.move.form</field>
            <field name="model">account.move</field>
            <field name="inherit_id" ref="account.view_move_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='invoice_origin']" position="attributes">
                    <!-- <attribute name="attrs">{}</attribute> -->
                    <attribute name="invisible"/>
                </xpath>
                <xpath expr="//form[1]/sheet[1]/notebook[1]/page[@name='other_info']/group[1]/group[@name='accounting_info_group']/field[@name='to_check']" position="after">
                    <field name="packing_list_id" options="{&quot;no_create&quot;:true}" invisible="packing_list_id == False"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>
