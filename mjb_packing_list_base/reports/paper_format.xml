<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="packing_list" model="report.paperformat">
        <field name="name">A4 Packing List</field>
        <field name="format">A4</field>
        <field name="orientation">Landscape</field>
        <field name="margin_top">40.00</field>
        <field name="margin_bottom">32.00</field>
        <field name="margin_left">7.00</field>
        <field name="margin_right">7.00</field>
        <field name="header_spacing">35</field>
        <field name="dpi">90</field>
    </record>

    <record id="action_packing_list_en" model="ir.actions.report">
        <field name="name">PDF Packing List</field>
        <field name="model">packing.list</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">mjb_packing_list_base.report_packing_list</field>
        <field name="print_report_name">'packing_list_%s' % (object.code)</field>
        <field name="paperformat_id" ref="mjb_packing_list_base.packing_list"/>
        <field name="binding_model_id" ref="model_packing_list"/>
        <field name="binding_type">report</field>
        <field name="binding_view_types">list,form</field>
    </record>
</odoo>