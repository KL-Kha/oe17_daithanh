<!--                        <field name="production_sheet_id"/>-->
<!--                        <field name="product_id"/>-->
<!--                        <field name="production_quantity_total"/>-->
<!--                        <field name="uom_id"/>-->
<!--                    </tree>-->
<!--                </field>-->
                <footer>
<!--                    <button name="mb.bsk_ps_to_pl_confirm" string="Confirm" type="action" class="btn-primary"/>-->
                    <button string="Cancel" class="btn-secondary" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>

    <record id="view_make_po_wiz_action" model="ir.actions.act_window">
        <field name="name">Purchase to Picking</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">wiz.purch.to.packing</field>
        <field name="view_mode">form</field>
        <field name="view_id" ref="wiz_purch_to_packing_form_view"/>
        <field name="target">new</field>
    </record>
