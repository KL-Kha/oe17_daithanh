<odoo>

    <record id="wiz_purch_to_packing_form_view" model="ir.ui.view">
        <field name="name">wiz.purch.to.packing.form.view</field>
        <field name="model">wiz.purch.to.packing</field>
        <field name="arch" type="xml">
            <form string="Ship goods">
                <h4>When you click confirm, it will add lines to the delivered list.</h4>
                <group>
                    <field name="name" invisible="1"/>
                    <field name="sheet_id" readonly="1"/>
<!--                    <field name="packing_list_type" required="1"/>-->
                    <field name="packing_list_id" options="{'no_create': True}"/>
