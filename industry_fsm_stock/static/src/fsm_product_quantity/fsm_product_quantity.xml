<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">

    <t t-inherit="industry_fsm_sale.FsmProductQuantity" t-inherit-mode="extension">
        <xpath expr="button[@name='fsm_remove_quantity']" position="attributes">
            <attribute name="t-att-disabled">props.record.data[props.name] === 0 or !props.record.data.quantity_decreasable</attribute>
        </xpath>
    </t>

</templates>
